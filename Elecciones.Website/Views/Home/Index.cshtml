@{
    ViewBag.Title = "Procesos";
}

@section Styles{
    <link href="~/Content/Site.css" rel="stylesheet" />

    <link rel="stylesheet" href="~/vendor/datatables.net-bs4/css/dataTables.bootstrap4.css">
    <link rel="stylesheet" href="~/vendor/datatables.net-keytable-bs/css/keyTable.bootstrap.css">
    <link rel="stylesheet" href="~/vendor/datatables.net-responsive-bs/css/responsive.bootstrap.css">
    <link rel="stylesheet" href="~/vendor/jquery-bootgrid/dist/jquery.bootgrid.css">

}



<div class="content-heading">Procesos de usuario</div>
<div class="form-group">
    <div class="input-group">
        <span class="input-group-addon" data-bind="text: 'Proceso'"></span>
        <select class="form-control" data-bind="  @*optionsCaption: '[Elige un Proceso]',*@ options: processList, optionsValue:'Id', optionsText: 'Name', value: selectedProcess"></select>
    </div>
</div>




