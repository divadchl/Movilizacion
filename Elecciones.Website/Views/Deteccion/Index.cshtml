
@{
    ViewBag.Title = "Personas";
}

@section Styles{
    <link href="~/Content/Site.css" rel="stylesheet" />

    <link rel="stylesheet" href="~/vendor/datatables.net-bs4/css/dataTables.bootstrap4.css">
    <link rel="stylesheet" href="~/vendor/datatables.net-keytable-bs/css/keyTable.bootstrap.css">
    <link rel="stylesheet" href="~/vendor/datatables.net-responsive-bs/css/responsive.bootstrap.css">
    <link rel="stylesheet" href="~/vendor/jquery-bootgrid/dist/jquery.bootgrid.css">
    <link rel="stylesheet" href="~/vendor/dropzone/dist/basic.css">
    <link rel="stylesheet" href="~/vendor/dropzone/dist/dropzone.css">


}


<div id="divPrincipal" data-bind="visible: $root.divPrincipalVisible">
    <div class="content-heading">Personas</div>
    <div class="form-group">
        <div class="input-group">
            <span class="input-group-addon" data-bind="text: 'Estado'"></span>
            <select class="form-control" data-bind="optionsCaption: '[Elige un Estado]', options: stateList, optionsValue:'Identifier', optionsText: 'State', value: state"></select>
        </div>
    </div>

    <div data-bind="visible: $root.state() > 0">
        <div class="checkbox">
            <label>
                <input type="checkbox" id="all" data-bind="checked: allChecked" />
                Mostrar Todos
                <i class="fa fa-square-o small"></i>
            </label>
        </div>
    </div>

    <div id="filter" class="box">

        <div class="box-header">
            <div class="box-name">
                <i class="fa fa-filter"></i>
                <span>Filtro de personas detectadas</span>
            </div>
        </div>
        <br />

        <div>
            <div class="form-group">
                <div class="input-group" data-bind="visible: filterList().length > 0 && $root.state() > 0">
                    <select class="form-control" data-bind="options: filterList, optionsValue: $data, optionsText: 'Description', optionsCaption: 'Agregar filtro:', value: selectedFilterList"></select>
                </div>
            </div>

            <div class="form-group" data-bind="visible: searchList().length > 0 && $root.state() > 0, foreach: searchList">
                <div>
                    <div class="input-group">
                        <span class="input-group-addon" data-bind="text: $data.description"></span>
                        <select class="form-control" data-bind="visible: false ,options: $data.comparerList, optionsValue: 'Comparador', optionsText: 'Descripcion', value: $data.selectedComparer"></select>
                    </div>
                </div>
                <div>
                    <!-- ko if: isTerritory() == true -->
                    <select class="form-control" data-bind="css: { error: $data.error() == true },options: $data.territoryList, optionsValue: 'Id', optionsText: 'Descripcion', optionsCaption: '[ELIGE UNA OPCIÓN]', value: $data.selectedValue"></select>
                    <!-- /ko -->
                    <!-- ko if: isTerritory() == false -->
                    <input class="form-control" type="text" data-bind="css: { error: $data.error() == true }, value: $data.selectedValue" />
                    <!-- /ko -->
                </div>
            </div>

            <div data-bind="visible: searchList().length  && $root.state() > 0">
                <div>
                    <a class="btn btn-oval btn-primary" data-bind="click: search">Buscar</a>
                    <a class="btn btn-oval btn-primary" data-bind="click: reset">Limpiar</a>
                </div>
            </div>

            <div class="clearfix"></div>
        </div>
        <br />
        <button class="btn btn-labeled btn-success mb-2" type="button" data-toggle="modal" data-target="#newPerson" data-bind="visible: $root.state() > 0, click: $root.initValuesNewPerson">
            <span class="btn-label">
                <i class="fa fa-plus"></i>
            </span>Nueva Persona
        </button>

        <div class="card">
            <div class="card-header">
                <div class="card-title">Información General</div>
                <div class="text-sm">Personas detectadas</div>
            </div>
            <div class="card-body">
                <table class="table table-bordered my-4 w-100" id="personTable">
                    <thead>
                        <tr>
                            <th>Nombre</th>
                            <th>Estado</th>
                            <th>Municipio</th>
                            <th>Distrito Federal</th>
                            <th>Distrito Local</th>
                            <th>Seccion</th>
                            <th>Vigente</th>
                            <th>Estatus</th>
                            <th>Opciones</th>
                        </tr>
                    </thead>
                    <tbody data-bind="foreach: $root.persons">
                        <tr class="gradeA">
                            <td data-bind="text: Nombre"></td>
                            <td data-bind="text: Estado"></td>
                            <td data-bind="text: Municipio"></td>
                            <td data-bind="text: DistritoFederal"></td>
                            <td data-bind="text: DistritoLocal"></td>
                            <td data-bind="text: Seccion"></td>
                            <td>
                                <ul>
                                    <li style="list-style: none">
                                        <div data-bind="css: VigenteColor, text: VigenteMessage "></div>
                                    </li>
                                </ul>
                            </td>
                            <td>
                                <ul>
                                    <li style="list-style: none">
                                        <div data-bind="css: EstatusColor, text: Estatus "></div>
                                    </li>
                                </ul>
                            </td>
                            <td>
                                <button type="button" class="btn btn-sm btn-success mr-2 command-edit" data-bind="click: $root.consult" title="Consultar"><em class="fa fa-user fa-fw"></em></button>
                                <button type="button" class="btn btn-sm btn-danger command-delete" data-bind="click: $root.locate" title="Ubicar" data-toggle="modal" data-target="#mapModalLocate"><em class="fa fa-map-marked-alt fa-fw"></em></button>
                            </td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>


        @section BodyArea {

            <div class="modal fade" id="mapModalLocate" tabindex="-1" role="dialog" aria-labelledby="myModalLabelLarge" aria-hidden="true">
                <div class="modal-dialog modal-lg">
                    <div class="modal-content">
                        <div class="modal-header">
                            <h4 class="modal-title" id="myModalLabelLarge">Ubicación</h4>
                            <button class="close" type="button" data-dismiss="modal" aria-label="Close">
                                <span aria-hidden="true">&times;</span>
                            </button>
                        </div>
                        <div class="modal-body">
                            <div id="locateMap" style="width: 100%; height: 600px;"></div>
                        </div>
                        <div class="modal-footer">
                            <button class="btn btn-secondary" type="button" data-dismiss="modal">Close</button>
                        </div>
                    </div>
                </div>
            </div>

            <div class="modal fade" id="photoContent" tabindex="-1" role="dialog" aria-labelledby="myModalLabelLarge" aria-hidden="true">
                <div class="modal-dialog modal-lg">
                    <div class="modal-content">
                        <div class="modal-header">
                            <h4 class="modal-title" id="myModalLabelLarge">Foto</h4>
                            <button class="close" type="button" data-dismiss="modal" aria-label="Close">
                                <span aria-hidden="true">&times;</span>
                            </button>
                        </div>
                        <div class="modal-body">
                            <img id="photoIne" style="display: block; width: 100%; height: 100%;" />
                        </div>
                        <div class="modal-footer">
                            <button class="btn btn-secondary" type="button" data-dismiss="modal">Close</button>
                        </div>
                    </div>
                </div>
            </div>

            <div class="modal fade" id="newPerson" tabindex="-1" role="dialog" aria-labelledby="myModalLabelLarge" aria-hidden="true">
                <div class="modal-dialog modal-xl">
                    <div class="modal-content">
                        <div class="modal-header">
                            <h4 class="modal-title" id="myModalLabelLarge"><span data-bind="text: $root.selectedPerson() != undefined ? 'Editar Persona' : 'Nueva Persona' "></span></h4>
                            <button class="close" type="button" data-dismiss="modal" aria-label="Close">
                                <span aria-hidden="true">&times;</span>
                            </button>
                        </div>
                        <div class="modal-body">
                            <div class="row">
                                <div class="col-lg-12">
                                    <div class="card card-default">
                                        <div class="card-header">
                                            <h3 class="card-title">INE</h3>
                                        </div>
                                        <div class="card-body">
                                            <div class="form-group">
                                                <div class="input-group">
                                                    <span class="input-group-addon" data-bind="text: 'Tipo de credencial'"></span>
                                                    <select class="form-control" data-bind="options: credentialTypeList, optionsValue:'Identifier', optionsText: 'Type', value: selectedCredentialType"></select>
                                                    <em class="fa-2x mr-2 fas fa-info-circle pointer btn-info" data-toggle="modal" data-target="#credentialType" style="visibility:hidden"></em>
                                                    <span></span>
                                                </div>

                                                <div id="collapseExample" class="accordion-toggle" data-bind="click: $root.ExpandCollapseExample, attr: { 'data-target': '#collapse1', 'data-toggle':  'collapse' }">
                                                    <div class="card-body d-flex align-items-center">
                                                        <em class="fa-1x mr-2 fas fa-plus" data-bind="css:{'fa-plus':  $root.IsExpandedExample() == true, 'fa-minus': $root.IsExpandedExample() == false  } "> Ejemplo </em>
                                                    </div>
                                                </div>

                                                <div class="row collapse" id="collapse1">
                                                    <div class="card-body col-md-10">
                                                        <img id="exampleIneFront" width="320" height="220" />
                                                        <img id="exampleIneBack" width="320" height="220" />
                                                    </div>
                                                </div>

                                            </div>

                                            <div class="form-group row">
                                                <label class="col-md-2 col-form-label">Foto de frente</label>
                                                <div class="col-md-10">
                                                    <input id="IneFront" accept="image/x-png,image/gif,image/jpeg" class="form-control filestyle" type="file" data-classbutton="btn btn-secondary" data-classinput="form-control inline"
                                                           data-icon="&lt;span class='fa fa-upload mr'&gt;&lt;/span&gt;" data-bind="event: { change: $root.setFrontIne }" />
                                                </div>
                                            </div>
                                            <div class="form-group row">
                                                <label class="col-md-2 col-form-label">Foto de atrás</label>
                                                <div class="col-md-10">
                                                    <input id="IneBack" class="form-control filestyle" type="file" data-classbutton="btn btn-secondary" data-classinput="form-control inline"
                                                           data-icon="&lt;span class='fa fa-upload mr'&gt;&lt;/span&gt;" data-bind="event: { change: $root.setBackIne }" />
                                                </div>
                                            </div>

                                            <div id="collapseSelected" class="accordion-toggle" data-bind="click: $root.ExpandCollapseSelected, attr: { 'data-target': '#collapse2', 'data-toggle':  'collapse' }">
                                                <div class="card-body d-flex align-items-center">
                                                    <em class="fa-1x mr-2 fas fa-plus" data-bind="css:{'fa-plus':  $root.IsExpandedSelected() == true, 'fa-minus': $root.IsExpandedSelected() == false  } "> Seleccionadas </em>
                                                </div>
                                            </div>

                                            <div class="row collapse" id="collapse2">
                                                <div class="card-body col-md-10">
                                                    <img id="selectedIneFront" width="320" height="220" />
                                                    <img id="selectedIneBack" width="320" height="220" />
                                                </div>
                                            </div>




                                            <button class="btn btn-labeled btn-success mb-2" type="button" data-bind="visible: $root.CredentialFrontPhotoLoadBase64() != undefined && $root.CredentialBackPhotoLoadBase64() != undefined,  click: $root.LoadIneInformation">
                                                <span class="btn-label">
                                                    <i class="fa fa-check"></i>
                                                </span>Cargar información
                                            </button>

                                            <div class="col-lg-12" data-bind="visible: $root.CredentialFrontPhotoLoadBase64() != undefined && $root.CredentialBackPhotoLoadBase64() != undefined">
                                                <div class="card card-default">
                                                    <div class="card-header">
                                                        <h3 class="card-title">Información de la persona</h3>
                                                    </div>
                                                    <div class="card-body">
                                                        <fieldset>
                                                            <div class="form-group row">
                                                                <label class="col-md-2 col-form-label">Nombre</label>
                                                                <div class="col-md-10">
                                                                    <input class="form-control col-lg-12" type="text" @*disabled=""*@ data-bind="value:$root.Nombre" />
                                                                    <span class="form-text"></span>
                                                                </div>
                                                            </div>
                                                            <div class="form-group row mb-2">
                                                                <label class="col-md-2 col-form-label">Paterno</label>
                                                                <div class="col-md-4">
                                                                    <input class="form-control col-lg-12" type="text" data-bind="value: $root.ApellidoPaterno" />
                                                                    <span class="form-text"></span>
                                                                </div>
                                                                <label class="col-md-2 col-form-label">Materno</label>
                                                                <div class="col-md-4">
                                                                    <input class="form-control col-lg-12" type="text" data-bind="value: $root.ApellidoMaterno" />
                                                                    <span class="form-text"></span>
                                                                </div>
                                                            </div>
                                                            <div class="form-group row mb-2">
                                                                <label class="col-md-2 col-form-label">Clave de Elector</label>
                                                                <div class="col-md-4">
                                                                    <input class="form-control col-lg-12" type="text" data-bind="value: $root.ClaveElector" />
                                                                    <span class="form-text"></span>
                                                                </div>
                                                                <label class="col-md-2 col-form-label">CIC</label>
                                                                <div class="col-md-4">
                                                                    <input class="form-control col-lg-12" type="text" data-bind="value: $root.CIC" />
                                                                    <span class="form-text"></span>
                                                                </div>
                                                            </div>
                                                            <div class="form-group row">
                                                                <label class="col-md-2 col-form-label">CURP</label>
                                                                <div class="col-md-10">
                                                                    <input class="form-control col-lg-12" type="text" data-bind="value: $root.CURP" />
                                                                    <span class="form-text"></span>
                                                                </div>
                                                            </div>
                                                            <div class="form-group row">
                                                                <label class="col-md-2 col-form-label">Calle</label>
                                                                <div class="col-md-10">
                                                                    <input class="form-control col-lg-12" type="text" data-bind="value: $root.Calle" />
                                                                    <span class="form-text"></span>
                                                                </div>
                                                            </div>

                                                            <div class="form-group row mb-2">
                                                                <label class="col-md-2 col-form-label">Colonia</label>
                                                                <div class="col-md-4">
                                                                    <input class="form-control col-lg-12" type="text" data-bind="value: $root.Colonia" />
                                                                    <span class="form-text"></span>
                                                                </div>
                                                                <label class="col-md-2 col-form-label">Localidad</label>
                                                                <div class="col-md-4">
                                                                    <input class="form-control col-lg-12" type="text" data-bind="value: $root.Localidad" />
                                                                    <span class="form-text"></span>
                                                                </div>

                                                            </div>

                                                            <div class="form-group row mb-2">
                                                                <label class="col-md-2 col-form-label">Municipio</label>
                                                                <div class="col-md-4">
                                                                    <input class="form-control col-lg-12" type="text" data-bind="value: $root.Municipio" />
                                                                    <span class="form-text"></span>
                                                                </div>
                                                                <label class="col-md-2 col-form-label">No. Municipio</label>
                                                                <div class="col-md-4">
                                                                    <input class="form-control col-lg-12" type="text" data-bind="value: $root.NoMunicipio" />
                                                                    <span class="form-text"></span>
                                                                </div>
                                                            </div>

                                                            <div class="form-group row mb-2">
                                                                <label class="col-md-2 col-form-label">Estado</label>
                                                                <div class="col-md-4">
                                                                    <input class="form-control col-lg-12" type="text" data-bind="value: $root.Estado" />
                                                                    <span class="form-text"></span>
                                                                </div>
                                                                <label class="col-md-2 col-form-label">No. Estado</label>
                                                                <div class="col-md-4">
                                                                    <input class="form-control col-lg-12" type="text" data-bind="value: $root.NoEstado" />
                                                                    <span class="form-text"></span>
                                                                </div>
                                                            </div>

                                                            <div class="form-group row mb-2">
                                                                <label class="col-md-2 col-form-label">Sección</label>
                                                                <div class="col-md-4">
                                                                    <input class="form-control col-lg-12" type="text" data-bind="value: $root.Seccion" />
                                                                    <span class="form-text"></span>
                                                                </div>
                                                                <label class="col-md-2 col-form-label">CP</label>
                                                                <div class="col-md-4">
                                                                    <input class="form-control col-lg-12" type="text" data-bind="value: $root.CP" />
                                                                    <span class="form-text"></span>
                                                                </div>
                                                            </div>

                                                            <div class="form-group row mb-2">
                                                                <label class="col-md-2 col-form-label">Emisión</label>
                                                                <div class="col-md-4">
                                                                    <input class="form-control col-lg-12" type="text" data-bind="value: $root.Emision" />
                                                                    <span class="form-text"></span>
                                                                </div>
                                                            </div>
                                                            <div class="form-group row">
                                                                <label class="col-md-2 col-form-label">Num. Emisión</label>
                                                                <div class="col-md-4">
                                                                    <input class="form-control col-lg-12" type="text" data-bind="value: $root.NumEmision" />
                                                                    <span class="form-text"></span>
                                                                </div>
                                                            </div>

                                                            <div class="form-group row  mb-2">
                                                                <label class="col-md-2 col-form-label">AñoRegistro</label>
                                                                <div class="col-md-4">
                                                                    <input class="form-control col-lg-12" type="text" data-bind="value: $root.AñoRegistro" />
                                                                    <span class="form-text"></span>
                                                                </div>
                                                                <label class="col-md-2 col-form-label">Vigencia</label>
                                                                <div class="col-md-4">
                                                                    <input class="form-control col-lg-12" type="text" data-bind="value: $root.Vigencia" />
                                                                    <span class="form-text"></span>
                                                                </div>
                                                            </div>

                                                            <div class="form-group row" style="visibility:hidden">
                                                                <label class="col-md-2 col-form-label">ZonaLecturaMecanica</label>
                                                                <div class="col-md-10">
                                                                    <input class="form-control col-lg-12" type="text" data-bind="value: $root.ZonaLecturaMecanica" />
                                                                    <span class="form-text"></span>
                                                                </div>
                                                            </div>
                                                        </fieldset>
                                                    </div>
                                                </div>
                                            </div>

                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="modal-footer">
                            <button class="btn btn-primary" type="button" data-bind="visible: $root.ClaveElector() != '', click: $root.personChanges">Guardar</button>
                            <button class="btn btn-secondary" type="button" data-dismiss="modal" data-bind="click: $root.clearNewPerson">Cancelar</button>
                        </div>
                    </div>
                </div>
            </div>

            <div class="modal fade" id="credentialType" tabindex="-1" role="dialog" aria-labelledby="myModalLabelLarge" aria-hidden="true">
                <div class="modal-dialog modal-lg">
                    <div class="modal-content">
                        <div class="modal-header">
                            <h4 class="modal-title" id="myModalLabelLarge"><span>Tipo de credenciales</span></h4>
                            <button class="close" type="button" data-dismiss="modal" aria-label="Close">
                                <span aria-hidden="true">&times;</span>
                            </button>
                        </div>
                        <div class="modal-body">
                            <div class="row">
                                <div class="col-lg-12">
                                    <div class="card card-default">
                                        <div class="card-body">
                                            <div class="col-lg-12">
                                                <div class="card card-default">
                                                    <div class="card-body">
                                                        <div class="card-header">
                                                            <h3 class="card-title">Tipo C</h3>
                                                        </div>
                                                        <img width="320" height="220" src="~/Content/Images/INE/TipoC_Front.jpg" />
                                                        <img width="320" height="220" src="~/Content/Images/INE/TipoC_Back.jpg" />
                                                    </div>
                                                    <div class="card-body">
                                                        <div class="card-header">
                                                            <h3 class="card-title">Tipo DEF</h3>
                                                        </div>
                                                        <img width="320" height="220" src="~/Content/Images/INE/TipoDEF_Front.jpg" />
                                                        <img width="320" height="220" src="~/Content/Images/INE/TipoDEF_Back.jpg" />
                                                    </div>
                                                    <div class="card-body">
                                                        <div class="card-header">
                                                            <h3 class="card-title">Tipo GH</h3>
                                                        </div>
                                                        <img width="320" height="220" src="~/Content/Images/INE/TipoGH_Front.jpg" />
                                                        <img width="320" height="220" src="~/Content/Images/INE/TipoGH_Back.jpg" />
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="modal-footer">
                            <button class="btn btn-secondary" type="button" data-dismiss="modal">Cerrar</button>
                        </div>
                    </div>
                </div>
            </div>
        }
    </div>
</div>

<div id="divCurrentPersonVisible" data-bind="visible: $root.divCurrentPersonVisible">
    <div class="content-heading">Detectadas - Detalle</div>
    <button class="btn btn-primary btn-sm" type="button" data-bind="click: $root.hideDetailPerson">
        <span class="btn-label">
            <i class="fa fa-arrow-left"></i>
        </span>Regresar
    </button>
    <div class="row">
        <div class="col-lg-6">
            <div class="card card-default">
                <div class="card-header">
                    <h3 class="card-title" data-bind="text: $root.Name"></h3>
                </div>
                <div class="card-body">
                    <img id="frontCredential" class="pointer" width="500" height="220" data-bind="click: $root.showPopPhoto(1)" data-toggle="modal" data-target="#photoContent" />
                    <br />
                    <br />
                    <img id="backCredential" class="pointer" width="500" height="220" data-bind="click: $root.showPopPhoto(2)" data-toggle="modal" data-target="#photoContent" />
                </div>
                <div class="card card-default">
                    <div class="card-header">
                        <h3 class="card-title">Invitados</h3>
                    </div>
                    <ul data-bind="foreach: $root.Guests">
                        <li style="list-style: none">
                            <div data-bind="css: IsRegistered ? 'badge badge-success' : 'badge badge-danger', text: IsRegistered ? 'Registrado' : 'No Registrado' "></div>
                            <label class="col-md-2 col-form-label" data-bind="text: Name"></label>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
        <div class="col-lg-6">
            <div class="card card-default">
                <div class="card-header">
                    <h3 class="card-title">Información de Contacto</h3>
                </div>
                <div class="card-body">
                    <fieldset>
                        <div class="form-group row">
                            <label class="col-md-2 col-form-label">Nombre</label>
                            <div class="col-md-10">
                                <input class="form-control col-lg-10" type="text" disabled="" data-bind="value:$root.Name" />
                                <span class="form-text"></span>
                            </div>
                        </div>
                        <div class="form-group row">
                            <label class="col-md-2 col-form-label">Correo</label>
                            <div class="col-md-10">
                                <input class="form-control col-lg-10" type="text" disabled="" data-bind="value: $root.Email" />
                                <span class="form-text"></span>
                            </div>
                        </div>
                        <div class="form-group row">
                            <label class="col-md-2 col-form-label">Teléfono</label>
                            <div class="col-md-10">
